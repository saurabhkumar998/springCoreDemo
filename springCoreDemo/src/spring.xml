<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE beans PUBLIC "-//SPRING//DTD BEAN 2.0//EN" "http://www.springframework.org/dtd/spring-beans-2.0.dtd">

<beans>
	<bean id ="triangle" class="com.spring.core.practice.Triangle">
		<property name ="pointA" ref="zeroPoint"/>
		<property name ="pointB">
				<bean class = "com.spring.core.practice.Point">
					<property name ="x" value="-20"/>
					<property name ="y" value="0"/>
				</bean>
		</property>
		<property name ="pointC" >
				<bean id ="point3" class = "com.spring.core.practice.Point">
					<property name ="x" value="20"/>
					<property name ="y" value="0"/>
				</bean>
		</property>
	</bean>
	
	<bean id ="zeroPoint" class = "com.spring.core.practice.Point">
		<property name ="x" value="0"/>
		<property name ="y" value="0"/>
	</bean>
	
	<alias name="triangle" alias="triangle-alias"/>
	

	

</beans>